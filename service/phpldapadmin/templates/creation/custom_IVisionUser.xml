<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE template SYSTEM "template.dtd">

<template>
<askcontainer>1</askcontainer>
<description>New Industrial Vision User</description>
<icon>address-book.png</icon>
<invalid>0</invalid>
<rdn>cn</rdn>
<!--<regexp>^ou=IVisionUsers,o=.*,</regexp>-->
<title>Industrial Vision User</title>
<visible>1</visible>

<objectClasses>
<objectClass id="ClassIndustrialVision"></objectClass>
</objectClasses>

<attributes>
<attribute id="cn">
	<display>Login</display>
	<order>1</order>
	<page>1</page>
	<spacer>1</spacer>
</attribute>
<attribute id="givenName">
	<display>First name</display>
	<onchange>=autoFill(cn;%givenName% %sn%)</onchange>
	<order>2</order>
	<page>1</page>
</attribute>
<attribute id="sn">
	<display>Last name</display>
	<onchange>=autoFill(cn;%givenName% %sn%)</onchange>
	<order>3</order>
	<page>1</page>
</attribute>
<attribute id="IVisionRole">
	<display>Industrial Vision Role</display>
	<order>4</order>
	<page>1</page>
</attribute>
<attribute id="IVisionPIN">
	<display>Industrioal Vision PIN</display>
	<order>5</order>
	<page>1</page>
</attribute>
</attributes>

</template>
